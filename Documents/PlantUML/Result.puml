@startuml

class ResultEntity {
    + int Turn 
    + int Sequence 
    + IResult Result 
}

interface IResult {
    
}

interface ISkillResult {
    + CharacterId CharacterId 
    + SkillCode SkillCode 
}

interface IDamageResult {
    + ImmutableList<DamageResultDetail> DamageResultDetailList 
}

together {
    class AilmentSkillResult {
        + CharacterId CharacterId 
        + SkillCode SkillCode 
        + bool Success 
        + ImmutableList<AilmentSkillResultDetail> AilmentSkillResultDetailList 
    }
    
    class DamageSkillResult {
        + CharacterId CharacterId 
        + SkillCode SkillCode 
        + ImmutableList<DamageResultDetail> DamageResultDetailList 
    }
}


class AilmentSkillResultDetail {
    + CharacterId CharacterId 
    + AilmentCode AilmentCode 
}

ResultEntity - IResult
IResult <|- ISkillResult
IResult <|- IDamageResult
ISkillResult <|- AilmentSkillResult
ISkillResult <|- DamageSkillResult
IDamageResult <|- DamageSkillResult
AilmentSkillResult - AilmentSkillResultDetail
@enduml